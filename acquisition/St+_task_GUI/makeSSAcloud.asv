function str=makeSSAcloud(s,d,p,nseq)

if nargin<3 || isempty(p),
p(1)=0.01;
p(2)=1-p(1);
end

fname=ls('c:\maestro_results\soundFiles\clouds\');


clear i t

i=strfind(fnames(ind,:),'_');
base=fname(ind,1:i(end));
clss=strcat(base,num2str(s));
clds=strcat(base,num2str(d));

str=[repmat(strcat(clss,','),[1 p(1)*nseq]) repmat(strcat(clds,','),[1 p(2)*nseq])];
str=str(1:end-1);
