function val = get(s_coord,prop_name)
% GET Get sig_coordinator property from the specified object
% and return the value. Property names are:
% 
switch prop_name
case 'Coordinator'
   val = s_coord.coordinator;  
   case 'M_sig'
   val = s_coord.coordinator;
otherwise
      val=get(s_coord.coordinator,prop_name);
%    val=get(get(s_coord,'Coordinator'),prop_name);
end